<div class="w3-myfont container">


    <h2 class="w3-myfont w3-padding">Rented Cars</h2>

    <table class="w3-table w3-margin">
        <th>
            Car
        </th>
        <th>
            Days
        </th>
        <th>
            Total price
        </th>

        <tr>
            {{#each rents}}
        <tr>
            <td>{{this.car.make}} {{this.car.model}}</td>

            <td>{{this.days}}</td>

            <td>{{this.totalPrice}}</td>

            <td> <a class="w3-btn w3-red w3-border w3-border-black w3-round" href="/cars/remove/{{this._id}}">Remove</a></td>
        </tr>



        {{/each}}




    </table>

</div>